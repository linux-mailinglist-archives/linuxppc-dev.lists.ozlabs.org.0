-					    const u8 *data,
-					    unsigned int len,
-					    sha256_block_fn *block_fn)
-{
-	unsigned int partial = sctx->count % SHA256_BLOCK_SIZE;
-
-	sctx->count += len;
-	BLOCK_HASH_UPDATE_BLOCKS(block_fn, &sctx->ctx, data, len,
-				 SHA256_BLOCK_SIZE, sctx->buf, partial);
-	return 0;
